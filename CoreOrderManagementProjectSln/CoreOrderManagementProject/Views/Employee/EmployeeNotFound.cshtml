@model int
@{ ViewBag.Title = "404 Error";}
<div class="alert alert-danger mt-1 mb-1">
    <h4>404  Not Found Error</h4>
    <h5>Employee with Id @Model cannot be found</h5>
</div>
<a asp-controller="Employee" asp-action="Index" class="btn btn-outline-success" style="width:auto">
    Click here to see all the employees
</a>